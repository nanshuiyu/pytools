<Scaffold>
  <ScaffoldFile Path="WebPICmdLine.exe" SourceDirectory="PythonSupportPath" TargetPath="bin\WebPICmdLine.exe" />
  <ScaffoldFile Path="wfastcgi.py" SourceDirectory="PythonSupportPath" TargetPath="bin\wfastcgi.py" />
  <ScaffoldFile Path="Microsoft.Web.Deployment.dll" SourceDirectory="PythonSupportPath" TargetPath="bin\Microsoft.Web.Deployment.dll" />
  <ScaffoldFile Path="Microsoft.Web.PlatformInstaller.dll" SourceDirectory="PythonSupportPath" TargetPath="bin\Microsoft.Web.PlatformInstaller.dll" />
  <ScaffoldFile Path="Microsoft.Web.PlatformInstaller.UI.dll" SourceDirectory="PythonSupportPath" TargetPath="bin\Microsoft.Web.PlatformInstaller.UI.dll" />
  <ScaffoldFile Path="Microsoft.PythonTools.WebRole.dll" SourceDirectory="PythonSupportPath" TargetPath="bin\Microsoft.PythonTools.WebRole.dll" />
  <ScaffoldFile Path="Microsoft.PythonTools.AzureSetup.exe" SourceDirectory="PythonSupportPath" TargetPath="bin\Microsoft.PythonTools.AzureSetup.exe" />
  <ScaffoldFile Path="AzureSetup.cfg" TargetPath="bin\AzureSetup.cfg">
    <Scaffold.ReplaceParameterRule />
  </ScaffoldFile>

  <ScaffoldFile Path="__init__.py" TargetPath="$RoleName$\__init__.py"/>
  <ScaffoldFile Path="settings.py" TargetPath="$RoleName$\settings.py">
    <Scaffold.ReplaceParameterRule />
  </ScaffoldFile>
  <ScaffoldFile Path="urls.py" TargetPath="$RoleName$\urls.py">
    <Scaffold.ReplaceParameterRule />
  </ScaffoldFile>
  <ScaffoldFile Path="manage.py" TargetPath="$RoleName$\manage.py"/>

  <ScaffoldFile Path="Web.cloud.config" />
  <ScaffoldFile Path="Web.config" />
  <ScaffoldFile Path="RoleSettings.xml" Copy="false">
    <Scaffold.ReplaceParameterRule />
    <PythonRules.AddRoleToConfig />
  </ScaffoldFile>
  <ScaffoldFile Path="WebRole.xml" Copy="false">
    <Scaffold.ReplaceParameterRule />
    <PythonRules.AddWebRoleToDef />
  </ScaffoldFile>
</Scaffold>
